{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true,
        "cypress/globals": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "cypress"
    ],
    "rules": {
        "react/prop-types": "off",
        "no-console": "off",
        "no-alert": "off",
        "no-case-declarations": "off",
        "no-param-reassign": "off",
        "no-unused-expressions": "off"
    }
}
